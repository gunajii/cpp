#include <iostream>
#include <string>
using namespace std;

class Game {
public:
    int n;
    int* score;
    void getdata();
    void sorting();
};

void Game::getdata() {
    cout << "Enter number of players" << endl;
    cin >> n;
    score = new int[n];
    cout << "Enter their respective scores" << endl;
    for (int k = 0; k < n; k++) {
        cin >> score[k];
    }
}

void Game::sorting() {
    int i, j, key;
    for (i = 0; i < n - 1; i++) {
        for (j = 0; j < n - i - 1; j++) {
            key = score[j];
            if (score[j] > score[j + 1]) {
                score[j] = score[j + 1];
                score[j + 1] = key;
            }
        }
    }
    for (int l = 0; l < n; l++) {
        cout << score[l] << " ";
    }
    cout << endl;
}

int main() {
    Game start;
    start.getdata();
    start.sorting();
    return 0;
}
